# AWS Credentials Template
# This file will be processed to ~/.aws/credentials

[default]
aws_access_key_id = ${AWS_ACCESS_KEY_ID}
aws_secret_access_key = ${AWS_SECRET_ACCESS_KEY}

# Optional: For temporary credentials
# aws_session_token = ${AWS_SESSION_TOKEN}

# Work profile (if applicable)
[work]
aws_access_key_id = ${AWS_WORK_ACCESS_KEY_ID}
aws_secret_access_key = ${AWS_WORK_SECRET_ACCESS_KEY}

# Personal profile
[personal]
aws_access_key_id = ${AWS_PERSONAL_ACCESS_KEY_ID}
aws_secret_access_key = ${AWS_PERSONAL_SECRET_ACCESS_KEY} 